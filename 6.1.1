@startuml
hide circle
hide empty methods

class ProgPrincipal {
  
}

class Qualification {
  code
  libelle
  tauxHoraire
}

class Artisan {
  SIRET
  nom
  adresse
  tph
}

class Tache {
  code
  designation
}

class Chantier {
  nomProprio
  adresseProprio
  adresseChantier
  telProprio
  dateOuverture
  dateFermeture
  
  estTermine()
  cloturer(Date)
  

}

class Modele {
  code
  designation
  prixVenteM
  prixRevientM
  dureeConstructionM
}


class Realiser {
  date
  duree
}

class Executer {
  ordre
  dureeMoy
}


Realiser "*" -up- "1" Artisan
Realiser "*" -down- "1" Chantier
Realiser "*" -left- "1" Tache
Tache "*" -up- "1" Qualification
Qualification "1" - "*" Artisan
Modele "0..*" - "1..*" Tache
Modele "1" -down- "*" Chantier
ProgPrincipal ---> "*" Chantier
ProgPrincipal ---> "*" Artisan
ProgPrincipal ---> "*" Modele

(Tache, Modele) .. Executer

@enduml
